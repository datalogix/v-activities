{
  "name": "v-activities",
  "description": "Activities for Vue 3",
  "repository": "datalogix/v-activities",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/v-activities.es.js",
      "require": "./dist/v-activities.umd.js"
    }
  },
  "main": "./dist/v-activities.umd.js",
  "module": "./dist/v-activities.es.js",
  "types": "./dist/v-activities.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typecheck && vite build",
    "prepublishOnly": "pnpm build",
    "dev": "vite --port 3333 --open",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "test": "pnpm lint && vitest",
    "test:unit": "pnpm test",
    "test:e2e": "cypress open",
    "cy": "cypress open-ct",
    "cy:ci": "cypress run-ct --quiet --reporter spec",
    "typecheck": "vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "release": "pnpm test && pnpm build && standard-version && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "arrows-svg": "^1.5.5",
    "eve": "^0.5.4",
    "lodash.shuffle": "^4.2.0",
    "print-js": "^1.6.0",
    "snapsvg": "^0.5.1"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.0.0",
    "@cypress/vue": "^4.0.0",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@iconify/json": "^2.1.63",
    "@types/jsdom": "^16.2.14",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/node": "^18.0.0",
    "@types/snapsvg": "^0.5.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.1",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.3",
    "cypress": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "jsdom": "^19.0.0",
    "sass": "^1.52.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.3",
    "unocss": "^0.39.0",
    "unplugin-icons": "^0.14.4",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.12",
    "vitepress": "^1.0.0-alpha.2",
    "vitest": "^0.15.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.37.9"
  },
  "packageManager": "pnpm@7.2.1"
}
